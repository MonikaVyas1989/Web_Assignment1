<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - MyWeb_Assignment</title>
    <link rel="stylesheet" href="~/css/site.css" />

    <link href="~/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />


</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <nav class="navbar">
                <div class="Container">
                    <a class="navbar-logo" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    <div class="navbar-link">
                        <ul>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="About">About</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Contact">Contact</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Projects">Projects</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Doctor" asp-action="FeverCheck">FeverCheck</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Game" asp-action="NumberGuessingGame">NumberGuessingGame</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="People" asp-action="PeopleIndex">People</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>

    </div>
    <div class="container body-content">
        <main role="main">
            @RenderBody()
        </main>
    </div>
    <div class="container">

        <hr />
        <footer class="footer fixed-bottom">

            @DateTime.Now.ToShortDateString() *&#169MonikaVyas*

        </footer>
    </div>
    
    
    <script src="~/lib/bootstrap/js/bootstrap.js"></script>
</body>

</html>
